        #<Fix Broken Shield Pools>

            # Various chests in the game have a possible "Shields" item pool which will
            # never actually trigger in-game because they include an invalid item in the
            # pool.  This fixes the pool so that it can be chosen again.

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix0"</key><value>",GD_Balance_Treasure.ChestGrades.ObjectGrade_DahlEpic,DefaultLoot[4].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix1"</key><value>",GD_Itempools.ListDefs.EpicChestRedLoot,LootData[4].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix2"</key><value>",GD_Ma_ItemPools.ListDefs.EpicChestRedLoot_Marigold,LootData[4].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix3"</key><value>",GD_Itempools.ListDefs.EpicChestHyperionLoot,LootData[3].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix4"</key><value>",GD_Ma_ItemPools.ListDefs.EpicChestHyperionLoot_Marigold,LootData[3].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

            #<hotfix><key>"SparkLevelPatchEntry-ApocEuphoriaChestFix5"</key><value>",GD_Meteorites.Balance.ObjectGrade_Meteorite_LootPile_Chest,DefaultLoot[4].ItemAttachments[11].ItemPool,,GD_Itempools.BuffDrinkPools.Pool_BuffDrinks_HealingRegen"</value><on>

        #</Fix Broken Shield Pools>

    #</Better Containers>

    #<Better Enemy Drops>

        #<Guaranteed Boss Drops>

            # Bosses should always drop their unique items

            set GD_Itempools.DropWeights.DropODDS_BossUniques:ConstantAttributeValueResolver_0 ConstantValue 1 

            set GD_Itempools.DropWeights.DropODDS_BossUniqueRares:ConstantAttributeValueResolver_0 ConstantValue 1 

        #</Guaranteed Boss Drops>

