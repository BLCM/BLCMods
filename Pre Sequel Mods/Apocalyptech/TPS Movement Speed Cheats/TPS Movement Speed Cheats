    #<Global Movement Vars>

        # These variables aren't player-specific, and don't seem to require hotfixes
        # to apply post-death-or-FFYL

        set GD_PlayerShared.injured.PlayerInjuredDefinition InjuredMovementSpeed 500

        set GD_Globals.General.Globals PlayerAirControl 0.9

    #</Global Movement Vars>

#</TPS Movement Speed Cheats>