#<TPS Movement Speed Cheats>

    # TPS Movement Speed Cheats v1.0.0
    # Licensed under Public Domain / CC0 1.0 Universal
    #
    # Ups the movement speed of all characters (including while crouched and during
    # FFYL), increases jump height a bit, and provides much more air control.

    #<Athena>

        # The set statements apply the settings initially

        set GD_Gladiator.Character.CharClass_Gladiator GroundSpeed 1000

        set GD_Gladiator.Character.CharClass_Gladiator AirSpeed 1100

        set GD_Gladiator.Character.CharClass_Gladiator CrouchedPct 1

        set GD_Gladiator.Character.CharClass_Gladiator JumpZ 900

        # The hotfixes apply once you respawn or exit FFYL
        # (These may be a bit overboard.  Not sure if you need the BaseValue
        # ones, or if the BaseValue ones are the only ones you need, etc.)

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaGroundSpeed0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,GroundSpeed,,1000"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaGroundSpeedBaseValue0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,GroundSpeedBaseValue,,1000"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaAirSpeed0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,AirSpeed,,1100"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaAirSpeedBaseValue0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,AirSpeedBaseValue,,1100"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaJumpZ0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,JumpZ,,900"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaJumpZBaseValue0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,JumpZBaseValue,,900"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAthenaCrouchedPct0"</key><value>"GD_Gladiator_Streaming,GD_Gladiator_Streaming.Pawn_Gladiator,CrouchedPct,,1"</value><on>

    #</Athena>

    #<Aurelia>

        # The set statements apply the settings initially

        set Crocus_Baroness.Character.CharClass_Baroness GroundSpeed 1000

        set Crocus_Baroness.Character.CharClass_Baroness AirSpeed 1100

        set Crocus_Baroness.Character.CharClass_Baroness CrouchedPct 1

        set Crocus_Baroness.Character.CharClass_Baroness JumpZ 900

        # The hotfixes apply once you respawn or exit FFYL
