        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAureliaGroundSpeedBaseValue0"</key><value>"Crocus_Baroness_Streaming,Crocus_Baroness_Streaming.Pawn_Baroness,GroundSpeedBaseValue,,1000"</value><on>

        #<hotfix><key>"SparkOnDemandPatchEntry-ApocAureliaAirSpeed0"</key><value>"Crocus_Baroness_Streaming,Crocus_Baroness_Streaming.Pawn_Baroness,AirSpeed,,1100"</value><on>

