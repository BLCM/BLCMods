						<code profiles="Solo">set GD_Soldier_Skills.Gunpowder.Battlefront SkillEffectPresentations (AttributePresentationDefinition'GD_Soldier_Skills.Gunpowder.Battlefront:AttributePresentationDefinition_9')</code>
					</hotfix>
				</category>
				<category name="Skill Icons">
					<hotfix name="AxtonRevisited" package="GD_Soldier_Streaming">
						<code profiles="Solo">set GD_Soldier_Skills.Gunpowder.DoOrDie SkillIcon SwfMovie'SharedSkillIcons_Soldier.SkillIcon-LastDitchEffort'</code>
						<code profiles="Solo">set GD_Soldier_Skills.Guerrilla.Onslaught SkillIcon SwfMovie'SharedSkillIcons_Soldier.SkillIcon-DoOrDie'</code>
						<code profiles="Solo">set GD_Soldier_Skills.Survival.LastDitchEffort SkillIcon SwfMovie'SharedSkillIcons_Soldier.SkillIcon-PickupTurret'</code>
