#<Fastball Launcher>

    #<Description>

        #by HolyHades

        #Breaks the KerBlaster

        #Replaces the Maggie

        #Shoots Fastball Grenades

        #Buffed Damage and Accuracy

        #Gold/Black/White Skin

        #Gibbed Code: BL2(hwAAAAD0wwCHRwVABgGFssOIhSEQowHG/////x0w/v8PxACDURHj)

        #FilterTool Support
		
	#</Description>

    #<Firing Mode>

        set GD_Weap_Pistol.Barrel.Pistol_Barrel_Jakobs_Maggie CustomFiringModeDefinition FiringModeDefinition'GD_Weap_AssaultRifle.FiringModes.FM_Rocket_KerBlaster'

        set GD_Weap_AssaultRifle.FiringModes.FM_Rocket_KerBlaster ProjectileDefinition GD_GrenadeMods.Projectiles.Grenade_Fastball 

        set GD_Weap_AssaultRifle.FiringModes.FM_Rocket_KerBlaster SpawnDirection DIRECTION_Random

    #</Firing Mode>

    #<Weapon Attributes>

        set GD_Weap_Pistol.Barrel.Pistol_Barrel_Jakobs_Maggie WeaponAttributeEffects ((AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponProjectilesPerShot',ModifierType=MT_PreAdd,BaseModifierValue=(BaseValueConstant=5.000000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)),(AttributeToModify=AttributeDefinition'D_Attributes.Weapon.WeaponDamage',ModifierType=MT_Scale,BaseModifierValue=(BaseValueConstant=1.200000,BaseValueAttribute=None,InitializationDefinition=None,BaseValueScaleConstant=1.000000)))

        set GD_Weap_Pistol.Barrel.Pistol_Barrel_Jakobs_Maggie AttributeSlotUpgrades ((SlotName="AccuracyMin",GradeIncrease=25,bActivateSlot=True),(SlotName="AccuracyMax",GradeIncrease=25,bActivateSlot=True),(SlotName="AccuracyRegen",GradeIncrease=25,bActivateSlot=True),(SlotName="WeaponAccuracyImpulse",GradeIncrease=25,bActivateSlot=True))

    #</Weapon Attributes>

    #<Name and Description>

        set GD_Weap_Pistol.Name.Title_Jakobs.Title_Legendary_Maggie PartName Fastball Launcher

        set GD_Weap_Pistol.Name.Title_Jakobs.Title_Legendary_Maggie:AttributePresentationDefinition_8 NoConstraintText Destruction.

    #</Name and Description>

    #<Skin>

        set GD_Weap_Pistol.ManufacturerMaterials.Mat_Jakobs_5_Legendary Material MaterialInstanceConstant 'Common_GunMaterials.Materials.Pistol.Mati_HyperionEpic'

    #</Skin>
	
#</Fastball Launcher>
