#<Electro>

#<Description>

##########################Electro##########################
#Electro                                                  #
#Replaces the Transformer                                 #
#Requires the Crawmerax HeadHunter Pack                   #
#Now drops from Sparky, Son of Flynt                      #
#Become a little more than immune to shock damage         #
#Goes well with the Tingling Sensation                    #
#Created by AngrierPat                                    #
###########################################################

#</Description>

#<Changes>

################################################################################################################################################

set GD_Shields.Titles.Title_Absorption04_AbsorptionShieldLegendaryShock PartName Electro

set GD_Shields.Titles.Title_Absorption04_AbsorptionShieldLegendaryShock:AttributePresentationDefinition_0 Description This is only the smallest fraction of my power.

set GD_Shields.Skills.Absorption_Shield_Skill_Legendary DamageEvents ((EventType=SDE_DamagedByEnemy,EventConstraints=((DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Impact',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False),(DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Status',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False)),EventName="Damaged By Enemy"),(EventType=SDE_DamagedByFriendly,EventConstraints=((DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Impact',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False),(DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Status',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False)),EventName="Damaged By Friendly"),(EventType=(INVALID),EventConstraints=((DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Impact',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False),(DamageTypeConstraint=WillowDamageTypeDefinition'GD_Shock.DamageType.DmgType_Shock_Status',DamageSourceConstraint=None,bMustBeCriticalDamage=False,bMustBeOneShotKill=False)),EventName="Damaged By Neutral"))

set GD_Shields.Capacitor.Capacitor5_Maliwan_ShockRes_Legendary AttributeSlotUpgrades ((SlotName="Capacity",GradeIncrease=3,bActivateSlot=False),(SlotName="RechargeRate",GradeIncrease=1,bActivateSlot=False),(SlotName="Special01",GradeIncrease=10,bActivateSlot=False),(SlotName="RechargeDelay",GradeIncrease=-3,bActivateSlot=False))

set GD_Shields.Capacitor.Capacitor5_Maliwan_ShockRes_Legendary:AttributePresentationDefinition_2 Description Grants a little more than immunity to shock damage.

set GD_Shields.A_Item.Shield_Absorption_05_LegendaryShock:AttributePresentationDefinition_1 Description Shield regenerates apon taking <font color="#13c0f9">shock damage or DoTs.

#</Changes>

