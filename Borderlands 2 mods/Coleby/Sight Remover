#<Sight Remover>

    Made by Coleby

    Removes the scope/sight attachment on Pistols, SMGs and Shotguns, because they don't look nice on these weapons.

    #<Bandit>

        #<Pistols>

            #<Common (White)>

                set GD_Weap_Pistol.A_Weapons.Pistol_Bandit:PartList SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_None',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=1)))

            #</Common (White)>

            #<Uncommon (Green)>

                set GD_Weap_Pistol.A_Weapons.Pistol_Bandit_2_Uncommon:WeaponPartListCollectionDefinition_13 SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_None',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=1)))

            #</Uncommon (Green)>

            #<Rare (Blue)>

                set GD_Weap_Pistol.A_Weapons.Pistol_Bandit_3_Rare:WeaponPartListCollectionDefinition_14 SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_None',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=1)))

            #</Rare (Blue)>

            #<Very Rare (Purple)>

                set GD_Weap_Pistol.A_Weapons.Pistol_Bandit_4_VeryRare:WeaponPartListCollectionDefinition_73 SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_None',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=1)))

            #</Very Rare (Purple)>

            #<E-Tech>

                set GD_Weap_Pistol.A_Weapons.Pistol_Bandit_5_Alien:PartList SightPartData (bEnabled=True,WeightedParts=((Part=WeaponPartDefinition'GD_Weap_Pistol.Sight.Pistol_Sight_None',Manufacturers=,MinGameStageIndex=0,MaxGameStageIndex=1,DefaultWeightIndex=1)))

            #</E-Tech>

            #<Unique>

